{
    "name": "dewsign/blockquote-repeater",
    "description": "Boilerplate repository for creating new Laravel or Javascript/Vue NPM packages",
    "type": "boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "laravel/nova": "^1.0||^2.0||^3.0",
        "dewsign/nova-repeater-blocks": "^1.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\BlockquoteRepeater\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\BlockquoteRepeater\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
